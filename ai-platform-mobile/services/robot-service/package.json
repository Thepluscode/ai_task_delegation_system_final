{
  "name": "robot-service",
  "version": "1.0.0",
  "description": "Robot service for automation AI platform",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "start:dev": "npx nest start --watch",
    "test": "jest",
    "debug": "ts-node -r tsconfig-paths/register src/debug-bootstrap.ts",
    "start:simple": "NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts --require=src/simplified-app.module.ts",
    "clean": "rm -rf node_modules dist package-lock.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/microservices": "^9.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "@tensorflow/tfjs-node": "^3.20.0",
    "compression": "^1.7.4",
    "helmet": "^6.0.0",
    "mongoose": "^6.6.0",
    "morgan": "^1.10.0",
    "nestjs-pino": "^3.1.0",
    "pino-pretty": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.0",
    "typeorm": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "jest": "^28.1.2",
    "pg": "^8.11.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.7.4"
  }
}